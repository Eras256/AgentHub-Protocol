{
  "name": "@agenthub/sdk",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for AgentHub Protocol - Build autonomous AI agents on Avalanche",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "pnpm run build",
    "cli": "tsx cli/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "agenthub",
    "avalanche",
    "ai",
    "agents",
    "blockchain",
    "x402",
    "defi",
    "ethereum",
    "web3"
  ],
  "author": {
    "name": "AgentHub Protocol",
    "url": "https://github.com/your-repo/agenthub-protocol"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/agenthub-protocol.git",
    "directory": "packages/agenthub-sdk"
  },
  "homepage": "https://github.com/your-repo/agenthub-protocol#readme",
  "bugs": {
    "url": "https://github.com/your-repo/agenthub-protocol/issues"
  },
  "license": "MIT",
  "dependencies": {
    "ethers": "^6.16.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "tsup": "^8.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "ethers": "^6.0.0"
  }
}
